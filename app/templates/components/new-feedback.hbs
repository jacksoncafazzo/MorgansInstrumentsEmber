{{#if newFeedbackForm}}
<form>
  <div class="form-group">
    <label for="title">Feedback Title</label>
    {{input value=title id="title"}}
  </div>
  <div class="form-group">
    <label for="content">Feedback</label>
    {{input value=content id="content"}}
  </div>
  <a class="btn" {{action 'saveFeedback' instrument}}>Save Feedback</a>
</form>
{{else}}
  <a class="btn" {{action 'feedbackForm'}}>Give Feedback</a>
{{/if}}
<a class="waves-effect waves-light btn modal-trigger" href="#modal1">Modal</a>

<div id="modal1" class="modal">
  <div class="modal-content">
    <h4>Modal Header</h4>
    <p>A bunch of text</p>
  </div>
  <div class="modal-footer">
    <a href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
  </div>
</div>
